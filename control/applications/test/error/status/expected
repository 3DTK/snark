# ensure that 1) output fields and format contain status and 2) the output for a reached waypoint ends with the reached flag set to 1
format/output="d,d,d,b"
fields/output="wayline/heading,error/cross_track,error/heading,reached"
binary/output[0]/target/heading_offset=1 +/- eps
binary/output[0]/target/speed=10 +/- eps
binary/output[0]/target/x=1 +/- eps
binary/output[0]/target/y=0 +/- eps
binary/output[0]/feedback/yaw=1.1 +/- eps
binary/output[0]/feedback/x=0 +/- eps
binary/output[0]/feedback/y=0 +/- eps
binary/output[0]/wayline/heading=0 +/- eps
binary/output[0]/error/cross_track=-0 +/- eps
binary/output[0]/error/heading=-0.1000000000000001 +/- eps
binary/output[0]/reached=0
binary/output[1]/target/heading_offset=1 +/- eps
binary/output[1]/target/speed=10 +/- eps
binary/output[1]/target/x=1 +/- eps
binary/output[1]/target/y=0 +/- eps
binary/output[1]/feedback/yaw=0.9 +/- eps
binary/output[1]/feedback/x=0.5 +/- eps
binary/output[1]/feedback/y=0.5 +/- eps
binary/output[1]/wayline/heading=0 +/- eps
binary/output[1]/error/cross_track=-0.5 +/- eps
binary/output[1]/error/heading=0.1000000000000001 +/- eps
binary/output[1]/reached=0
binary/output[2]/target/heading_offset=1 +/- eps
binary/output[2]/target/speed=10 +/- eps
binary/output[2]/target/x=1 +/- eps
binary/output[2]/target/y=0 +/- eps
binary/output[2]/feedback/yaw=1 +/- eps
binary/output[2]/feedback/x=1 +/- eps
binary/output[2]/feedback/y=0 +/- eps
binary/output[2]/wayline/heading=0 +/- eps
binary/output[2]/error/cross_track=0 +/- eps
binary/output[2]/error/heading=0 +/- eps
binary/output[2]/reached=1
binary/output[3]/target/heading_offset=2 +/- eps
binary/output[3]/target/speed=20 +/- eps
binary/output[3]/target/x=2 +/- eps
binary/output[3]/target/y=0 +/- eps
binary/output[3]/feedback/yaw=2.1 +/- eps
binary/output[3]/feedback/x=1.5 +/- eps
binary/output[3]/feedback/y=-0.5 +/- eps
binary/output[3]/wayline/heading=0 +/- eps
binary/output[3]/error/cross_track=0.5 +/- eps
binary/output[3]/error/heading=-0.1000000000000001 +/- eps
binary/output[3]/reached=0
binary/output[4]/target/heading_offset=2 +/- eps
binary/output[4]/target/speed=20 +/- eps
binary/output[4]/target/x=2 +/- eps
binary/output[4]/target/y=0 +/- eps
binary/output[4]/feedback/yaw=2 +/- eps
binary/output[4]/feedback/x=2 +/- eps
binary/output[4]/feedback/y=0 +/- eps
binary/output[4]/wayline/heading=0 +/- eps
binary/output[4]/error/cross_track=0 +/- eps
