#-----------------------------------------------------------------------------
#
# rur_config.cmake - rur CMake configuration file for external projects.
#
# This file is configured by rur and used by the use_rur.cmake module
# to load rur's settings for an external project.
@rur_CONFIG_INSTALL_ONLY@

SET(rur_INCLUDE_DIRS "@rur_INCLUDE_DIRS_CONFIG@")

SET(rur_LIBRARY_DIRS "@rur_LIBRARY_DIRS_CONFIG@")

SET( rur_ALL_LIBRARIES "@rur_ALL_LIBRARIES@" )
SET( rur_ALL_EXTERNAL_LIBRARIES "@rur_ALL_EXTERNAL_LIBRARIES@" )

# The rur binary executable directories.  Note that if
# rur_CONFIGURATION_TYPES is set (see below) then these directories
# will be the parent directories under which there will be a directory
# of runtime binaries for each configuration type.
SET(rur_EXECUTABLE_DIRS "@rur_EXECUTABLE_DIRS_CONFIG@")

# The rur runtime library directories.  Note that if
# rur_CONFIGURATION_TYPES is set (see below) then these directories
# will be the parent directories under which there will be a directory
# of runtime libraries for each configuration type.
SET(rur_RUNTIME_LIBRARY_DIRS "@rur_RUNTIME_LIBRARY_DIRS_CONFIG@")

# The runtime library path variable name e.g. LD_LIBRARY_PATH,
# this environment variable should be set to rur_RUNTIME_LIBRARY_DIRS
SET(rur_RUNTIME_PATH_VAR_NAME "@SHARED_LIBRARY_PATH_VAR_NAME@")

SET(rur_REQUIRED_C_FLAGS "@rur_REQUIRED_C_FLAGS@")
SET(rur_REQUIRED_CXX_FLAGS "@rur_REQUIRED_CXX_FLAGS@")
SET(rur_REQUIRED_EXE_LINKER_FLAGS "@rur_REQUIRED_EXE_LINKER_FLAGS@")
SET(rur_REQUIRED_SHARED_LINKER_FLAGS "@rur_REQUIRED_SHARED_LINKER_FLAGS@")
SET(rur_REQUIRED_MODULE_LINKER_FLAGS "@rur_REQUIRED_MODULE_LINKER_FLAGS@")

SET(rur_MAJOR_VERSION "@rur_MAJOR_VERSION@")
SET(rur_MINOR_VERSION "@rur_MINOR_VERSION@")
SET(rur_BUILD_VERSION "@rur_BUILD_VERSION@")

SET(rur_USE_FILE "@rur_USE_FILE@")

SET(rur_BUILD_SHARED_LIBS "@BUILD_SHARED_LIBS@")

IF( WIN32 )
  ADD_DEFINITIONS( -DNOMINMAX )
ENDIF(WIN32)

INCLUDE_DIRECTORIES(${rur_INCLUDE_DIRS})
LINK_DIRECTORIES(${rur_LIBRARY_DIRS})
