help/command[0]="image-color-calc --help"
help/command[1]="image-color-calc --help --verbose"

query/command[0]="image-color-calc --from rgb --input-fields"
query/command[1]="image-color-calc --from ycbcr --input-fields"
query/command[2]="image-color-calc --from ypbpr --input-fields"

query/command[3]="image-color-calc --to rgb --output-fields"
query/command[4]="image-color-calc --to ycbcr --output-fields"
query/command[5]="image-color-calc --to ypbpr --output-fields"
