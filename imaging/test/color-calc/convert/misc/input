help/command[0]="image-color-calc --help"
help/command[1]="image-color-calc --help --verbose"

query/command[0]="image-color-calc convert --from rgb --input-fields"
query/command[1]="image-color-calc convert --from ycbcr --input-fields"
query/command[2]="image-color-calc convert --from ypbpr --input-fields"

query/command[3]="image-color-calc convert --to rgb --output-fields"
query/command[4]="image-color-calc convert --to ycbcr --output-fields"
query/command[5]="image-color-calc convert --to ypbpr --output-fields"

failure/command[0]="image-color-calc --from rgb --input-fields"
failure/command[1]="image-color-calc cvert --from rgb --input-fields"
