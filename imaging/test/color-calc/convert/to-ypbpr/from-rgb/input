ascii/command[0]="echo 0,0,0 | image-color-calc convert --from rgb --to ypbpr --format=3f"
ascii/command[1]="echo 1,1,1 | image-color-calc convert --from rgb --to ypbpr --format=3f"

ascii/command[2]="echo 0,0,0 | image-color-calc convert --from rgb --to ypbpr --format=3d"
ascii/command[3]="echo 1,1,1 | image-color-calc convert --from rgb --to ypbpr --format=3d"

ascii/command[4]="echo 0.5,0.5,0.5 | image-color-calc convert --fields r,g,b --to ypbpr --format=3d"
ascii/command[5]="echo name,0.5,0.5,20170102T030405,0.5 | image-color-calc convert --fields name,r,g,t,b --to ypbpr --format=s[10],2d,t,d"

failure/command[0]="echo 1,1,1 | image-color-calc convert --from rgb --to ypbpr"
failure/command[1]="echo 1,1,1 | image-color-calc convert --from rgb --to ypbpr --format=3ub"

binary/command[0]="echo 0.5,0.5,0.5 | csv-to-bin 3f | image-color-calc convert --fields r,g,b --to ypbpr --binary=3f | csv-from-bin 3f,3f"
binary/command[1]="echo name,0.5,0.5,20170102T030405,0.5 | csv-to-bin s[10],2f,t,f | image-color-calc convert --fields name,r,g,t,b --to ypbpr --binary=s[10],2f,t,f" | csv-from-bin s[10],2f,t,f,3f
