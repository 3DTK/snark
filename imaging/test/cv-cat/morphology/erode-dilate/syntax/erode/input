circle[4]="cv-cat --file ../data/square.png 'erode=circle,7,'   | wc -c"
circle[5]="cv-cat --file ../data/square.png 'erode=circle,'     | wc -c"

cross[0]="cv-cat --file ../data/square.png 'erode=cross,,,,'      | wc -c"
cross[1]="cv-cat --file ../data/square.png 'erode=cross,7,9,,'    | wc -c"
cross[2]="cv-cat --file ../data/square.png 'erode=cross,,9,,'     | wc -c"
