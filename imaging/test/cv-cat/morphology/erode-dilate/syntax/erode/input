ellipse[5]="cv-cat --file ../data/square.png 'erode=ellipse,7,,2,3'   | wc -c"
ellipse[6]="cv-cat --file ../data/square.png 'erode=ellipse,7,11,2,3' | wc -c"
ellipse[7]="cv-cat --file ../data/square.png 'erode=ellipse,,11'      | wc -c"

circle[0]="cv-cat --file ../data/square.png 'erode=circle,,'    | wc -c"
