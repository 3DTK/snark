none[0]="cv-cat --file ../data/square.png 'erode' | wc -c"

rectangle[0]="cv-cat --file ../data/square.png 'erode=rectangle,,,,'    | wc -c"
rectangle[1]="cv-cat --file ../data/square.png 'erode=rectangle,3,4,,'  | wc -c"
rectangle[2]="cv-cat --file ../data/square.png 'erode=rectangle,,4,,'   | wc -c"
rectangle[3]="cv-cat --file ../data/square.png 'erode=rectangle,3,,,'   | wc -c"
rectangle[4]="cv-cat --file ../data/square.png 'erode=rectangle,3,,-1,' | wc -c"
rectangle[5]="cv-cat --file ../data/square.png 'erode=rectangle,3,,2,3' | wc -c"
rectangle[5]="cv-cat --file ../data/square.png 'erode=rectangle,3,5,2,3' | wc -c"

square[0]="cv-cat --file ../data/square.png 'erode=square,,'    | wc -c"
square[1]="cv-cat --file ../data/square.png 'erode=square,3,2'  | wc -c"
square[2]="cv-cat --file ../data/square.png 'erode=square,,-1'  | wc -c"
square[3]="cv-cat --file ../data/square.png 'erode=square,,1'   | wc -c"
square[4]="cv-cat --file ../data/square.png 'erode=square,3,'   | wc -c"

ellipse[0]="cv-cat --file ../data/square.png 'erode=ellipse,,,,'    | wc -c"
ellipse[1]="cv-cat --file ../data/square.png 'erode=ellipse,3,4,,'  | wc -c"
ellipse[2]="cv-cat --file ../data/square.png 'erode=ellipse,,4,,'   | wc -c"
ellipse[3]="cv-cat --file ../data/square.png 'erode=ellipse,3,,,'   | wc -c"
ellipse[4]="cv-cat --file ../data/square.png 'erode=ellipse,3,,-1,' | wc -c"
ellipse[5]="cv-cat --file ../data/square.png 'erode=ellipse,3,,2,3' | wc -c"
ellipse[5]="cv-cat --file ../data/square.png 'erode=ellipse,3,5,2,3' | wc -c"

circle[0]="cv-cat --file ../data/square.png 'erode=circle,,'    | wc -c"
circle[1]="cv-cat --file ../data/square.png 'erode=circle,3,2'  | wc -c"
circle[2]="cv-cat --file ../data/square.png 'erode=circle,,-1'  | wc -c"
circle[3]="cv-cat --file ../data/square.png 'erode=circle,,1'   | wc -c"
circle[4]="cv-cat --file ../data/square.png 'erode=circle,3,'   | wc -c"

cross[0]="cv-cat --file ../data/square.png 'erode=cross,,,,'    | wc -c"
cross[1]="cv-cat --file ../data/square.png 'erode=cross,3,4,,'  | wc -c"
cross[2]="cv-cat --file ../data/square.png 'erode=cross,,4,,'   | wc -c"
cross[3]="cv-cat --file ../data/square.png 'erode=cross,3,,,'   | wc -c"
cross[4]="cv-cat --file ../data/square.png 'erode=cross,3,,-1,' | wc -c"
cross[5]="cv-cat --file ../data/square.png 'erode=cross,3,,2,3' | wc -c"
cross[5]="cv-cat --file ../data/square.png 'erode=cross,3,5,2,3' | wc -c"

